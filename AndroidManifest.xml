<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cb"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.cb.ui.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.cb.xmlparser.ui.XmlParserActivity"
            android:label="@string/xml_parser_name" >
        </activity>
        <activity
            android:name="com.cb.fragmenttest.ui.FragmentTestActivity"
            android:label="@string/fragment_test_name" >
        </activity>
        
        <activity
            android:name="com.cb.web.ui.WebActivity"
            android:label="@string/web_activity" >
        </activity>
        
    </application>

</manifest>